                      font-weight: 400
                      letter-spacing: .75px
                      overflow: hidden
                      text-overflow: ellipsis
                      text-shadow: 2px 2px rgba(80,80,80,.25)
                      white-space: nowrap
                    text: ='Day ' + items[props.day].displayState
