timestamp: Sep 13, 2025, 3:58:16 PM
component: f7-card
config:
  title: VMC Ideo 325
slots:
  default:
    - component: f7-row
      slots:
        default:
          - component: f7-col
            config:
              style:
                align-items: baseline
                display: flex
                gap: 0px
            slots:
              default:
                - component: f7-icon
                  config:
                    material: access_time
                    size: 32
                    style:
                      margin-right: 10px
                    visible: true
                - component: Label
                  config:
                    style:
                      margin-right: 30px
                    text: Auto1
                - component: f7-icon
                  config:
                    material: wind_power
                    size: 32
                    style:
                      margin-right: 10px
                    visible: =items[props.vacancyMode].state=='OFF'
                - component: f7-icon
                  config:
                    material: wind_power
                    size: 32
                    style:
                      margin-right: 30px
                    visible: =items[props.vacancyMode].state=='OFF' && items[props.fanMode].state==0
                - component: f7-icon
                  config:
                    material: hide_source
                    size: 32
                    style:
                      margin-right: 10px
                    visible: =items[props.bypassMode].state!=0
                - component: f7-icon
                  config:
                    material: severe_cold
                    size: 32
                    style:
                      margin-right: 30px
                    visible: false
                - component: f7-icon
                  config:
                    material: beach_access
                    size: 32
                    style:
                      margin-right: 30px
                    visible: =items[props.vacancyMode].state=='ON'
                - component: f7-icon
                  config:
                    material: build
                    size: 32
                    style:
                      margin-right: 10px
                    visible: false
                - component: f7-icon
                  config:
                    material: report_problem
                    size: 32
                    style:
                      margin-right: 30px
                    visible: false
                - component: f7-icon
                  config:
                    material: wifi
                    size: 32
                    style:
                      margin-right: 10px
                    visible: true
    - component: f7-row
      slots:
        default:
          - component: f7-col
            config:
              style:
                align-items: baseline
                display: flex
                gap: 10px
                margin-top: 20px
            slots:
